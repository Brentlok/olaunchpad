{
  "expo": {
    "name": "Olaunchpad",
    "slug": "olanuchpad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.brentlok.olaunchpad",
      "intentFilters": [
        {
          "action": "android.intent.action.ASSIST",
          "category": [
            "android.intent.category.DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "activityTheme": "@android:style/Theme.DeviceDefault.Light.Dialog.NoActionBar",
            "launchMode": "singleTask"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f23ceb47-483c-4cff-a453-aad90e29144e"
      }
    }
  }
}
